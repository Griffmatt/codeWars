function findOutlier(integers){
  let odds = []
  let evens = []
  for(i = 0; i < integers.length; i++){
    if(integers[i] % 2 === 0 ){
      evens.push(integers[i])
    }
    if(integers[i] % 2 !== 0 ){
      odds.push(integers[i])
    }
  if(evens.length === 1 && odds.length > 1) return evens[0]
    if(odds.length === 1 && evens.length > 1 ) return odds[0]
   }
}
